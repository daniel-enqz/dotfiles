% Git
# Change branch
git checkout <branch>
$ branch: git branch | awk '{print $NF}'

% Brew
# Update brew
brew update

# Upgrade brew packages
brew upgrade

% Zim
# Upgrade Zim
zimfw upgrade

# Update Zim Modules
zimfw update

# New Zim Module Added
zimfw uninstall && zimfw install

% Localhost
# Kill Server
kill -9 <PID>
$ PID: lsof -wni tcp:3000 | awk '{print $2}'

% Postgres
# Start Postgres
rm /opt/homebrew/var/postgres/postmaster.pid
brew services restart postgresql
