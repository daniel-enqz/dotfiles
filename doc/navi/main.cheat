% Git
# Change branch
git checkout <branch>
$ branch: git branch | awk '{print $NF}'

% Brew
# Update brew
echo brew update

# Upgrade brew packages
echo brew upgrade

% Zim
# Upgrade Zim
echo zimfw upgrade

# Update Zim Modules
echo zimfw update

# New Zim Module Added
echo zimfw uninstall && zimfw install

% Localhost
# Kill Server
kill -9 <PID>
$ PID: lsof -wni tcp:3000 | awk '{print $2}'
